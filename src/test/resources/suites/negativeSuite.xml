<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Negative" parallel="methods" data-provider-thread-count="4">
    <listeners>
        <listener class-name="net.absoft.Listeners.TestListener"></listener>
        <listener class-name="net.absoft.Listeners.AnnotationTransformer"></listener>
    </listeners>
    <test name="authServiceTest">
        <groups>
            <run>
                <include name="negative"/>
            </run>
        </groups>
    </test>
    <packages>
        <package name="net.absoft"></package>
    </packages>
</suite>